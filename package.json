{
  "name": "laze-website",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "yarn pathpida -w",
    "test": "vitest --no-threads",
    "coverage": "vitest run --no-threads --coverage",
    "build": "yarn pathpida && next build",
    "start": "next start",
    "pathpida": "pathpida -p .gitignore -o ./src/lib -s",
    "lint": "next lint --dir src",
    "stylelint": "stylelint --ignore-path .gitignore './src/**/*.{css,scss,ts,tsx}'",
    "fix": "yarn lint --fix && yarn stylelint --fix && yarn format",
    "typecheck": "tsc",
    "prettier": "prettier --config .prettierrc.json './**/*.{js,jsx,ts,tsx,json,md}'",
    "format": "yarn prettier --write",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mdx-js/loader": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.0",
    "@next/mdx": "^13.4.2",
    "@reduxjs/toolkit": "^1.9.5",
    "firebase": "^9.22.0",
    "gray-matter": "^4.0.3",
    "i18next": "^22.4.15",
    "next": "^13.4.2",
    "next-i18next": "^13.2.2",
    "next-mdx-remote": "^4.4.1",
    "nprogress": "^0.2.0",
    "postcss": "8.4.23",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-i18next": "^12.2.2",
    "react-jdenticon": "^1.0.1",
    "react-redux": "^8.0.5",
    "react-toastify": "^9.1.3",
    "ress": "^5.0.2",
    "sharp": "^0.32.1",
    "swr": "^2.1.5",
    "ts-pattern": "^4.3.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@emotion/babel-plugin": "^11.11.0",
    "@storybook/addon-actions": "^7.0.11",
    "@storybook/addon-essentials": "^7.0.11",
    "@storybook/addon-interactions": "^7.0.11",
    "@storybook/addon-links": "^7.0.11",
    "@storybook/addon-toolbars": "^7.0.11",
    "@storybook/builder-webpack5": "^7.0.11",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^7.0.11",
    "@storybook/testing-library": "^0.1.0",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "20.1.4",
    "@types/nprogress": "^0.2.0",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-c8": "^0.31.0",
    "babel-loader": "^9.1.2",
    "eslint": "^8.40.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "^13.4.2",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-mdx": "^2.1.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-testing-library": "^5.11.0",
    "happy-dom": "^9.18.3",
    "husky": "^8.0.3",
    "hygen": "^6.2.11",
    "lint-staged": "^13.2.2",
    "msw": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.18.1",
    "postcss-jsx": "^0.36.4",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.8.8",
    "storybook-addon-next": "^1.8.0",
    "stylelint": "^15.6.1",
    "stylelint-config-idiomatic-order": "^9.0.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-prettier": "^3.0.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.5",
